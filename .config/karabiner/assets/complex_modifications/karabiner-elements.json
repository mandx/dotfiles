{
  "rules": [
    {
      "description": "Remap `Home` to `Cmd+Left` and `End` to `Cmd+Right`, `Shift` variant included",
      "manipulators": [
        {
          "conditions": [
            {
              "bundle_identifiers": [
                "^org\\.vim\\.",
                "^com.googlecode.iterm2"
              ],
              "type": "frontmost_application_unless"
            }
          ],
          "from": {
            "key_code": "end"
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "command"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "conditions": [
            {
              "bundle_identifiers": [
                "^org\\.vim\\.",
                "^com.googlecode.iterm2"
              ],
              "type": "frontmost_application_unless"
            }
          ],
          "from": {
            "key_code": "home"
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "command"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "conditions": [
            {
              "bundle_identifiers": [
                "^org\\.vim\\.",
                "^com.googlecode.iterm2"
              ],
              "type": "frontmost_application_unless"
            }
          ],
          "from": {
            "key_code": "end",
            "modifiers": {
              "mandatory": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "command",
                "shift"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "conditions": [
            {
              "bundle_identifiers": [
                "^org\\.vim\\.",
                "^com.googlecode.iterm2"
              ],
              "type": "frontmost_application_unless"
            }
          ],
          "from": {
            "key_code": "home",
            "modifiers": {
              "mandatory": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "command",
                "shift"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ],
  "title": "Generic tweaks"
}
